#include <stdio.h>

int maiorDeTres(int a, int b, int c) {
    int maior = a;
    if (b > maior) {
        maior = b;
    }
    if (c > maior) {
        maior = c;
    }
    return maior;
}

int main() {
    int num1, num2, num3;
    printf("Digite três números: ");
    scanf("%d %d %d", &num1, &num2, &num3);

    int resultado = maiorDeTres(num1, num2, num3);
    printf("O maior número é: %d\n", resultado);

    return 0;
}

// Esse bagulho parece facil, a pergunta nº 6 é curtinha, da a impressão que ta mais tranquilo, TA NADA!!
